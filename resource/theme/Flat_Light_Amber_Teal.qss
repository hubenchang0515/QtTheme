* {
    margin: 0;
    padding: 0;
    border: 0;
}

QWidget {
    background-color: #fafafa;
    color: #424242;
}

QWidget:disabled {
    background-color: #fafafa;
    color: #bdbdbd;
}



QWidget[color="Primary"] {
    background-color: #fafafa;
    color: #ffca28;
}

QWidget[color="Primary"]:disabled {
    background-color: #fafafa;
    color: #ffecb3;
}



QWidget[color="Secondary"] {
    background-color: #fafafa;
    color: #26a69a;
}

QWidget[color="Secondary"]:disabled {
    background-color: #fafafa;
    color: #b2dfdb;
}


QCheckBox::indicator {
    image: url(":/icon/check_box_unchecked_#424242.svg");
    color: #424242;
}

QCheckBox::indicator:checked {
    image: url(":/icon/check_box_checked_#424242.svg");
    color: #424242;
}

QCheckBox::indicator:disabled {
    image: url(":/icon/check_box_unchecked_#bdbdbd.svg");
    color: #424242;
}



QCheckBox[color="Primary"]::indicator {
    image: url(":/icon/check_box_unchecked_#ffca28.svg");
    color: #ffca28;
}

QCheckBox[color="Primary"]::indicator:checked {
    image: url(":/icon/check_box_checked_#ffca28.svg");
    color: #ffca28;
}

QCheckBox[color="Primary"]::indicator:disabled {
    image: url(":/icon/check_box_unchecked_#ffe082.svg");
    color: #ffe082;
}



QCheckBox[color="Secondary"]::indicator {
    image: url(":/icon/check_box_unchecked_#26a69a.svg");
    color: #26a69a;
}

QCheckBox[color="Secondary"]::indicator:checked {
