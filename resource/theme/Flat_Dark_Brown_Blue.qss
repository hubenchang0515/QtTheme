* {
    margin: 0;
    padding: 0;
    border: 0;
}

QWidget {
    background-color: #212121;
    color: #f5f5f5;
}

QWidget:disabled {
    background-color: #212121;
    color: #9e9e9e;
}



QWidget[color="Primary"] {
    background-color: #212121;
    color: #795548;
}

QWidget[color="Primary"]:disabled {
    background-color: #212121;
    color: #4e342e;
}



QWidget[color="Secondary"] {
    background-color: #212121;
    color: #2196f3;
}

QWidget[color="Secondary"]:disabled {
    background-color: #212121;
    color: #1565c0;
}


QCheckBox::indicator {
    image: url(":/icon/check_box_unchecked_#f5f5f5.svg");
    color: #f5f5f5;
}

QCheckBox::indicator:checked {
    image: url(":/icon/check_box_checked_#f5f5f5.svg");
    color: #f5f5f5;
}

QCheckBox::indicator:disabled {
    image: url(":/icon/check_box_unchecked_#9e9e9e.svg");
    color: #f5f5f5;
}



QCheckBox[color="Primary"]::indicator {
    image: url(":/icon/check_box_unchecked_#795548.svg");
    color: #795548;
}

QCheckBox[color="Primary"]::indicator:checked {
