* {
    margin: 0;
    padding: 0;
    border: 0;
}

QWidget {
    background-color: #fafafa;
    color: #424242;
}

QWidget:disabled {
    background-color: #fafafa;
    color: #bdbdbd;
}



QWidget[color="Primary"] {
    background-color: #fafafa;
    color: #42a5f5;
}

QWidget[color="Primary"]:disabled {
    background-color: #fafafa;
    color: #bbdefb;
}



QWidget[color="Secondary"] {
    background-color: #fafafa;
    color: #ffa726;
}

QWidget[color="Secondary"]:disabled {
    background-color: #fafafa;
    color: #ffe0b2;
}


QCheckBox::indicator {
    image: url(":/icon/check_box_unchecked_#424242.svg");
    color: #424242;
}

QCheckBox::indicator:checked {
    image: url(":/icon/check_box_checked_#424242.svg");
    color: #424242;
}

QCheckBox::indicator:disabled {
    image: url(":/icon/check_box_unchecked_#bdbdbd.svg");
    color: #424242;
}



QCheckBox[color="Primary"]::indicator {
    image: url(":/icon/check_box_unchecked_#42a5f5.svg");
    color: #42a5f5;
}

QCheckBox[color="Primary"]::indicator:checked {
    image: url(":/icon/check_box_checked_#42a5f5.svg");
    color: #42a5f5;
}

QCheckBox[color="Primary"]::indicator:disabled {
    image: url(":/icon/check_box_unchecked_#90caf9.svg");
    color: #90caf9;
}



QCheckBox[color="Secondary"]::indicator {
    image: url(":/icon/check_box_unchecked_#ffa726.svg");
    color: #ffa726;
}

QCheckBox[color="Secondary"]::indicator:checked {
