QDateEdit {
    color: {{base[2]}};
    background-color: {{base[8]}};
    border: 1px solid {{base[5]}};
    padding: 0.5em 1em;
}

QDateEdit:disabled {
    color: {{base[5]}};
    background-color: {{base[8]}};
    border: 1px solid {{base[6]}};
    padding: 0.5em 1em;
}

QDateEdit::up-button,
QDateEdit::down-button {
    width: 24px;
    background-color: {{base[8]}};
}

QDateEdit::up-button:hover,
QDateEdit::down-button:hover {
    width: 24px;
    background-color: {{base[6]}};
}

QDateEdit::up-button:pressed ,
QDateEdit::down-button:pressed  {
    width: 24px;
    background-color: {{base[7]}};
}

QDateEdit::up-button:disabled ,
QDateEdit::down-button:disabled  {
    width: 24px;
    background-color: {{base[8]}};
}

QDateEdit::up-arrow {
    width: 24px;
    image: url(":/icon/chevron_up/{{base[5]}}.svg");
}

QDateEdit::down-arrow {
    width: 24px;
    image: url(":/icon/chevron_down/{{base[5]}}.svg");
}

QDateEdit::up-arrow:disabled,
QDateEdit::up-arrow:off {
    width: 24px;
    image: url(":/icon/chevron_up/{{base[6]}}.svg");
}

QDateEdit::down-arrow:disabled,
QDateEdit::down-arrow:off {
    width: 24px;
    image: url(":/icon/chevron_down/{{base[6]}}.svg");
}

{% for key in colors %}

QDateEdit[Color="{{key}}"] {
    color: {{colors[key][6]}};
    background-color: {{base[8]}};
    border: 1px solid {{colors[key][6]}};
    padding: 0.5em 1em;
}

QDateEdit[Color="{{key}}"]:disabled {
    color: {{colors[key][7]}};
    background-color: {{base[8]}};
    border: 1px solid {{colors[key][8]}};
    padding: 0.5em 1em;
}

QDateEdit[Color="{{key}}"]::up-arrow {
    width: 24px;
    image: url(":/icon/chevron_up/{{colors[key][5]}}.svg");
}

QDateEdit[Color="{{key}}"]::down-arrow {
    width: 24px;
    image: url(":/icon/chevron_down/{{colors[key][5]}}.svg");
}

QDateEdit[Color="{{key}}"]::up-arrow:disabled,
QDateEdit[Color="{{key}}"]::up-arrow:off {
    width: 24px;
    image: url(":/icon/chevron_up/{{colors[key][8]}}.svg");
}

QDateEdit[Color="{{key}}"]::down-arrow:disabled,
QDateEdit[Color="{{key}}"]::down-arrow:off {
    width: 24px;
    image: url(":/icon/chevron_down/{{colors[key][8]}}.svg");
}

{% endfor %}